<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>	
	</head>
<body>

	<!-- '课件台 Report Main' Page -->

	<div data-weblab-type="template" name="labReport/showLabReport">
		
		<div data-weblab-type="labReportTitle"></div>
		<div data-weblab-type="labReportSubtitle"></div>


		<div data-weblab-type="labReportContent" id="labReportDisplay"  class="jsVerticallyCentered">			
			<div class="experimentTakeAways">
				<!-- Orchestra -->
				<div class="experiment" id="orchestra">
					<div class="title">
						<div class="weblabIcon outlineIcon orchestra"></div>
						<h4>UNIVERSAL ORCHESTRA</h4>
					</div>
					<div class="thumbnails">
						<!-- Takeaway thumbnails go in here -->
						<div class="thumbnail placeholderBox empty">
							<div class="promptHolder">
								<div class="prompt">
									<p class="small empty">Create music with Orchestra</p>									
								</div>
							</div>
						</div>
					</div>

					<div class="spinnerLayer white">
						<div class="spinnerSmall"></div>
					</div>

					<div class="nav">
						<button class="forward"></button>
						<button class="back"></button>						
					</div>
					
				</div>

				<!-- Periscopes -->
				<div class="experiment" id="periscopes">
					<div class="title">
						<div class="weblabIcon outlineIcon periscopes"></div>
						<h4>TELEPORTER</h4>
					</div>
					<div class="thumbnails">
						<!-- Takeaway thumbnails go in here -->
						<div class="thumbnail placeholderBox empty"><div class="promptHolder"><div class="prompt"><p class="small">Take a snapshot with Teleporter</p></div></div></div>
					</div>

					<div class="spinnerLayer white">
						<div class="spinnerSmall"></div>
					</div>

					<div class="nav">
						<button class="forward"></button>
						<button class="back"></button>		
					</div>
				</div>

				<!-- Sand Robots -->
				<div class="experiment" id="sandRobots">
					<div class="title">
						<div class="weblabIcon outlineIcon robots"></div>
						<h4>SKETCHBOTS</h4>
					</div>
					<div class="thumbnails">
						<!-- Takeaway thumbnails go in here -->
						<div class="thumbnail placeholderBox empty"><div class="promptHolder"><div class="prompt"><p class="small">Get your portrait drawn with Sketchbots</p></div></div></div>
					</div>

					<div class="spinnerLayer white">
						<div class="spinnerSmall"></div>
					</div>

					<div class="nav">
						<button class="forward"></button>
						<button class="back"></button>		
					</div>
				</div>

				<!-- Data Visualiser -->
				<div class="experiment" id="visualiser">
					<div class="title">
						<div class="weblabIcon outlineIcon visualiser"></div>
						<h4>DATA TRACER</h4>
					</div>
					<div class="thumbnails">
						<!-- Takeaway thumbnails go in here -->
						<div class="thumbnail placeholderBox empty"><div class="promptHolder"><div class="prompt"><p class="small">Create a traceroute with Data Tracer</p></div></div></div>
					</div>

					<div class="spinnerLayer white">
						<div class="spinnerSmall"></div>
					</div>

					<div class="nav">
						<button class="forward"></button>
						<button class="back"></button>		
					</div>
				</div>
			</div>

			<div class="footer">
				<div class="social">
					
				</div>
				<div id="deleteAllContent">
					<div class="deleteAllContainer">
						<a href=""><p class="small">Delete all</p></a>
						<button class="weblabColourGreyDark delete"></button>
					</div>
				</div>
				
			</div>

		</div>

	</div>

	<!-- 'Video Processing' thumbnail template -->
	<div data-weblab-type="template" name="labReport/videoProcessingThumbnail">
		<div class="processingPrompt">
			<div class="mask top">
				<div class="placeholderBoxContent"/>
			</div>
			<div class="mask bottom">
				<div class="placeholderBoxContent"/>
			</div>
			<h6>VIDEO PROCESSING</h6>			
		</div>
	</div>

	<div data-weblab-type="template" name="labReport/videoProcessingThumbnailOrchestra">
		<div class="processingPrompt orchestra">			
			<h6>VIDEO PROCESSING</h6>			
		</div>
	</div>

	<div data-weblab-type="template" name="labReport/videoProcessingThumbnailSketchbots">
		<div class="processingPrompt sketchbots">			
			<h6>VIDEO PROCESSING</h6>			
		</div>
	</div>


	<!-- 'Confirm Save TakeAway' Page -->

	<div data-weblab-type="template" name="labReport/labReportConfirmSaveTakeAway">
		
		<div data-weblab-type="labReportTitle"><h1>{Orchestra_live_takeaway_save_page_title}</h1></div>
		<div data-weblab-type="labReportSubtitle">
			<div class="confirmSave">
				<div class="spinnerCopy hidden"><p>Saving to your 课件台访客. 
<br />
It will be ready soon.</p></div>
				<p class="saveSubtitle">

				</p>	

				<div class="thumbnailHolder">
						<!-- Thumbnail content gets loaded in here -->	

					<div class="spinner black hidden"></div>
				</div>

				
				
				<div class="decisionButtons">
					
					<button class="weblabColourGreyMid" id="doNotSave"><h4>NO</h4></button>
					<button class="weblabGradientGreen" id="yesDoSave"><h4>YES, SAVE TO 课件台访客</h4></button>
				</div>

				<div data-weblab-type="userLoginPrompt" class="userPrompt">
					
					
				</div>
			</div>	
		</div>

		<div data-weblab-type="labReportContent" id="confirmSaveTakeAwayContent"></div>			
			
	</div>

	<!-- 'Create YouTube Channel' Page -->

	<div data-weblab-type="template" name="labReport/labReportCreateYouTubeChannel">
		
		<div data-weblab-type="labReportTitle"><h1>Create a YouTube Channel</h1></div>
		<div data-weblab-type="labReportSubtitle">		
			<div class="createChannelContent">
				<p class="instructions">In order to save your video you'll need to create a YouTube channel.
<br /><br />
Please enter a channel name below.</p>
				<div class="field">					
									
					<input id="usernameInputField" filled="false" name="username" type="text" maxlength="80"></input>

					<div id="usernameStatusBox" class="">
						<div class="checking"></div>
						<div class="tick"></div>
						<div class="nope"></div>						
					</div>	

					<div class="suggestionsContainer">					
						<div id="suggestionsBox">
							<div class="label"><p class="small">Suggestions:</p></div>
							<ul class="suggestionsList">
								<li><p>lauren002</p></li>
								<li><p>lauren_lauren_1</p></li>
								<li><p>lauren_vids</p></li>
								<li><p>lauren_videos</p></li>
								<li><p>lauren2300</p></li>
							</ul>
						</div>	
					</div>				
				</div>
			</div>
			<div class="decisionButtons">	
					<div class="spinnerSmall black hidden"></div>				
					<button class="weblabColourGreyMid" id="cancel"><h4>CANCEL</h4></button>
					<button class="weblabGradientGreen inactive" id="yesDoSave"><h4>CREATE &amp; SAVE</h4></button>					
				</div>
		</div>

		<div data-weblab-type="labReportContent" id="confirmSaveTakeAwayContent">
			
		</div>			
			
	</div>


	<div data-weblab-type="template" name="labReport/isLoggedInPrompt">
		<div class="prompt loggedIn">
			<p class="small">You are signed in as [insert user e-mail address]</p>
			<p class="small"><a href="#" class="signOutLink small">Not you?</a></p>
		</div>
	</div>

	<div data-weblab-type="template" name="labReport/isNotLoggedInPrompt">
		<div class="prompt notLoggedIn">
			<p class="small">To save everything you create you will need a Google Account. You'll be prompted to sign in or sign up.</p>
		</div>
	</div>


	<!-- 'Show TakeAway Content' Page -->

	<div data-weblab-type="template" name="labReport/showTakeAwayContent">
		
		<div data-weblab-type="labReportTitle"><h1>YOUR 课件台 REPORT</h1></div>
		<div data-weblab-type="labReportSubtitle">			
			<div class="decisionButtons">
				<button class="weblabColourGreyMid" id="backToOverview"><h4>VIEW 课件台 REPORT</h4></button>				
			</div>
		</div>
		
		
		<div data-weblab-type="labReportContent" id="showTakeAway"  class="jsVerticallyCentered">
				<div class="itemCarousel">
					<div class="nav">		
						<button class="back"></button>
						<button class="forward"></button>
					</div>
					<div class="takeAwayMask">
						<div class="takeAway">
						
						</div>	
					</div>
					<div class="spinnerLayer visible">
						<div class="spinner black"></div>
					</div>
				</div>

				
		</div>	

	</div>

	<div data-weblab-type="template" name="labReport/showTakeAwayContentSharing">
		
		<div data-weblab-type="labReportTitle"><h1>YOUR 课件台 REPORT</h1></div>
		<div data-weblab-type="labReportSubtitle" class="sharing">			
			<p>Your creation has been saved to your 课件台访客. Now share it with your friends.</p>
			<div class="sharingDecisionButtons">			
					<button class="weblabColourGreyDark" id="backToOverview"><h4>VIEW 课件台 REPORT</h4></button>
					<button class="weblabGradientGreen" id="done"><h4>DONE</h4></button>
				</div>	
		</div>
		
		<div data-weblab-type="labReportContent" id="showTakeAway"  class="jsVerticallyCentered sharing">
			<div class="itemCarousel">
				<div class="nav">		
					<button class="back"></button>
					<button class="forward"></button>
				</div>
				<div class="takeAwayMask">
					<div class="takeAway">
						
					</div>	
				</div>


				

				<div class="spinnerLayer visible">
						<div class="spinner black"></div>
				</div>
			</div>
		</div>	

	</div>

	

	<!-- Takeaway content display fragment -->

	<div data-weblab-type="template" name="labReport/takeAwayContentView" class="takeAwayContentView">
		<div class="group">
			<div class="content">
				<!-- Actual takeaway content goes in here -->
			</div>	
			<div class="description">
				<div class="detail"></div>
				<div class="weblabIcon avatarIcon"></div>
			</div>
		</div>

		<div class="actionButtons">
			<div class="social">				
				
			</div>
			<button class="weblabColourGreyDark delete"></button>
			<button class="weblabColourGreyDark flag"></button>
		</div>	
	</div>

	<div data-weblab-type="template" name="labReport/takeAwayShareView" class="takeAwayContentView sharing">
		<div class="group">
			<div class="content">
				<!-- Actual takeaway content goes in here -->
			</div>	
			<div class="description">
				<div class="detail"></div>
				<div class="weblabIcon avatarIcon"></div>
			</div>
		</div>

		<div class="actionButtons">
			<div class="social">				
				
			</div>
			<button class="weblabColourGreyDark delete"></button>
			<button class="weblabColourGreyDark flag"></button>
		</div>	
	</div>

	<div data-weblab-type="template" name="labReport/socialPanel" class="socialPanel">

		<div class="share-rule">
			<h4>SHARE THIS</h4>
			<hr class="blackSinglePixel"/>
		</div>

		<div class="buttons">
			<!-- Google -->		
			<div class="g-plusone" data-size="medium" data-annotation="bubble"></div>
			<!-- Facebook -->
			<div class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
			<!-- Twitter -->
			<!-- <a href="https://twitter.com/share" class="twitter-share-button" data-lang="en">Tweet</a> -->
			<iframe class="twitter-share-button" allowtransparency="true" frameborder="0" scrolling="no" src="//platform.twitter.com/widgets/tweet_button.html" style="width:91px; height:20px;"></iframe>
		</div>
		<div class="link">
			<!-- URL popout -->
			<div class="linkPopout">
				<input class="takeAwayShare" value="" name="takeAwayShare" readonly="readonly" maxlength="30"/>
				<button class="linkPreview"/>
			</div>
		</div>
	</div>

	<div data-weblab-type="template" name="labReport/visualiserOnlineContent" class="visualiserContent online">	
		<img class="backgroundImage"/>					
		
		<a href="#" class="imageLink" target="_blank">
			<div class="searchImageContainer">			
				<img class="searchImage"/>
				<p class="imageLinkText small"></p>
			</div>
		</a>

		<div class="spinnerHolder">
			<div class="spinner black"></div>
		</div>
		
	</div>

	<div data-weblab-type="template" name="labReport/visualiserMuseumContent" class="visualiserContent museum">			
		
		<div class="searchImageContainer">			
			<a href="#" class="imageLink" target="_blank"><img class="searchImage"/>
				<p class="imageLinkText small"></p></a>
		</div>
		<img class="worldMap"/>
	

	</div>


	<div data-weblab-type="template" name="labReport/orchestraLiveDescription">	
		<span class="collaborationLogo"></span>
		<h4>RECORDED LIVE AT THE SCIENCE MUSEUM</h4>
		<span class="small collaborationText">This was a real-time collaboration with people across the world.</span>		
	</div>

	<div data-weblab-type="template" name="labReport/orchestraVirtualDescription">	
		<span class="collaborationLogo"></span>
		<h4>RECORDED ONLINE</h4>
		<span class="small collaborationText">This was a real-time collaboration with people across the world.</span>		
	</div>



	<div data-weblab-type="template" name="labReport/periscopesDescription" class="periscopesDescription">		
		<div class="floatLeft colLocation">
    		<span class="date black"></span><h3 class="location"></h3>
    	</div>
    	<div class="floatLeft colTakenFrom">
    		<span class="takenFrom">Taken live from [Insert location of user]</span>        	
    	</div>
	</div>

	

	<div data-weblab-type="template" name="labReport/sandRobotsDescription">	
		<div class="floatLeft">
    		<span class="black date"></span><h4 class="robotInfo">PORTRAIT BY SKETCHBOT [INSERT NUMBER]</h4> 
    	</div>	
       		     
	</div>

	<div data-weblab-type="template" name="labReport/sandRobotsVectorDescription">	
		<div class="floatLeft">
    		<span class="black date"></span><h4 class="robotInfo">SKETCHBOT PORTRAIT</h4> 
    	</div>	
       		     
	</div>



	<div data-weblab-type="template" name="labReport/visualiserDescription" class="visualiserDescription">	
		<div class="floatLeft">	
			<div class="descriptionCopy">
				<span class="topDescription"><p class="small black">I searched for this image on the web and discovered where in the world it's stored. 
<div class="spacer"></div>
Located on a server in:</p></span>
				<span class="location"></span>
			</div>

			<div class="timeHolder">
				<span class="tracerouteCopy small black">Traceroute took:</span>
				<span class="traceRouteTime universFont">0.627</span><span class="seconds black">Sec</span>
			</div>
			<!-- <div class="comparison">
				<img class="visualiserImage" src=""/>
		       	<span class="visualiserComparisonText small"></span>
		    </div> -->
	   	</div>
	</div>

	<div data-weblab-type="template" name="labReport/visualiserDescriptionDummy" class="visualiserDescription">		
		<div class="floatLeft">				
			<div class="comparison">
				<img class="visualiserImage" src="/files/images/visualiser/labreport/speedIcons/space-station.png"/>
		       	<span class="visualiserComparisonText small">That's 3201x faster than the International Space Station orbiting Earth!</span>
		    </div>
	   	</div>
	</div>

	<div data-weblab-type="template" name="labReport/virtualOrchestraNoChrome">	
		<div class="noChromeDialog">
			<h2>YOUR BROWSER DOESN'T SUPPORT THIS</h2>
			<hr class="blackSinglePixel line"/>
			<p>Web 课件台 is powered by the latest web technologies and is best experienced in Chrome. Your current browser doesn't allow you to view this content</p>
			<div class="downloadChrome"><a class="splashButton" href="https://www.google.com/chrome/intl/en-GB/p/weblab.html" target="_blank">Download Chrome</a><img src="/files/images/home/chrome.png" alt="Chrome"/></div>			
		</div>
	</div>


	<div data-weblab-type="template" name="labReport/reportVideoProcessingPrompt" class="processingHolder">		
			<div class="centeredDialogHolder">
				<div class="promptDialog">	
					<div class="message">										
						<p class="small">Enter your email address to be notified when it's ready to watch.</p>
						<div class="emailField">							
							<input type="text" name="emailAddress" filled="false" value=""></input>	
							<div id='emailTickBox' tick='true' visible='false'></div>						
						</div>
					</div>
				</div>
			</div>			
	</div>

	<div data-weblab-type="template" name="labReport/reportVideoProcessing" class="processingHolder">	
		<div class="centeredDialogHolder">			
			<div class="centeredDialog">	
				<div class="message">					
					<h5>VIDEO PROCESSING</h5>
					<p class="small">It will be ready soon.</p>
				</div>	
			</div>
		</div>		
	</div>

	<div data-weblab-type="template" name="labReport/reportVideoError" class="processingHolder">	
		<div class="centeredDialogHolder">			
			<div class="centeredDialog">	
				<div class="message">					
					<h5>VIDEO PROCESSING ERROR</h5>
					<p class="small">We're very sorry, but there was a problem creating this video.</p>
				</div>	
			</div>
		</div>		
	</div>

	<div data-weblab-type="template" name="labReport/itemError" class="errorHolder">	
		<div class="centeredDialogHolder">			
			<div class="centeredDialog">	
				<div class="message">										
					<p class="small">Could not display this content</p>
				</div>	
			</div>
		</div>		
	</div>


	<div data-weblab-type="template" name="labReport/confirmDeleteAllDialog" class="dialog">
		<div class="title">
			<div class="animationLayer"><h1>DELETE ALL?</h1></div>
		</div>
		<hr class="blackSinglePixel line"/>
		<div class="content">
			<div class="animationLayer">
				<p>Do you want to delete everything within your 课件台 Report? It will be lost forever.</p>
				<br/>
				<button class="cancelButton button weblabColourGreyDark">
					<h4>NO, THANKS</h4>
				</button>
				<button class="confirmButton button weblabGradientGreen">
					<h4>DELETE ALL</h4>
				</button>
				<div class="clearBoth"></div>
			</div>
		</div>
	</div>


	<div data-weblab-type="template" name="labReport/confirmDeleteSingleDialog" class="dialog">
		<div class="title">
				<div class="animationLayer"><h1>DELETE THIS?</h1></div>
			</div>
			<hr class="blackSinglePixel line"/>
			<div class="content">
				<div class="animationLayer">
					<p>Are you sure you want to delete this?</p>
					<br/>
					<button class="cancelButton button weblabColourGreyDark">
						<h4>No</h4>
					</button>
					<button class="confirmButton button weblabGradientGreen">
						<h4>Yes</h4>
					</button>
					<div class="clearBoth"></div>
				</div>
			</div>
	</div>

	<div data-weblab-type="template" name="labReport/confirmFlagContentDialog" class="dialog">
		<div class="title">
				<div class="animationLayer"><h1>FLAG THIS?</h1></div>
			</div>
			<hr class="blackSinglePixel line"/>
			<div class="content">
				<div class="animationLayer">
					<p>Is this content inappropriate? Flag it and it will be reviewed for moderation.</p>
					<br/>
					<button class="cancelButton button weblabColourGreyDark">
						<h4>CANCEL</h4>
					</button>
					<button class="confirmButton button weblabGradientGreen">
						<h4>FLAG IT</h4>
					</button>
					<div class="clearBoth"></div>
				</div>
			</div>
	</div>

	<!-- 课件台 Repor Error Page -->

	<div data-weblab-type="template" name="labReport/labReportErrorPage">
		
		<div data-weblab-type="labReportTitle"><h1>ERROR</h1></div>
		<div data-weblab-type="labReportSubtitle" class="mine">			
			<p>Could not load your 课件台 Report</p>
		</div>
		<div data-weblab-type="labReportSubtitle" class="others">			
			<p>Could not load this 课件台 Report</p>
		</div>			
		
		<div data-weblab-type="labReportContent">								
		</div>	

	</div>

	<!-- 'Show TakeAway Content' Error Page -->

	<div data-weblab-type="template" name="labReport/showTakeAwayContentError">
		
		<div data-weblab-type="labReportTitle"><h1>ERROR</h1></div>
		<div data-weblab-type="labReportSubtitle">	
			<div class="decisionButtons">
				<button class="weblabColourGreyMid" id="backToOverview"><h4>VIEW 课件台 REPORT</h4></button>				
			</div>
		</div>
		
		
		<div data-weblab-type="labReportContent" id="showTakeAway"  class="jsVerticallyCentered sharing">
				<div class="itemCarousel">
					<div class="takeAwayMask">
						<div class="takeAway" style="opacity:1">
							<div class="takeAwayContentView" style="top:0px;">
								<div class="errorHolder">
									<div class="centeredDialogHolder">			
										<div class="centeredDialog">	
											<div class="message">										
												<p class="small">Could not display this content</p>
											</div>	
										</div>
									</div>
								</div>
							</div>	
						</div>	
					</div>
				</div>

				
		</div>	

	</div>

</body>
</html>
