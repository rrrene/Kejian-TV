.overnav {
    position: absolute;
    top: 30px;
    bottom: 35px;
    left: 0;
    right: 0;
    z-index: 2000;
}
.overnav a {
    outline: 0;
}
.overnav a.prev {
    position: absolute;
    display: block;
    top: 0;
    left: 0;
    height: 100%;
    width: 50%;
    background: "<%=asset_path('embed_back.png')%>";
    text-indent: -9999px;
    overflow: hidden;
}
.overnav a.next {
    position: absolute;
    display: block;
    top: 0;
    left: 50%;
    right: 0;
    bottom: 0;
    background: "<%=asset_path('embed_forward.png')%>";
    text-indent: -9999px;
    overflow: hidden;
}
.share .overnav {
    display: none;
}
.navigation {
    position: absolute;
    bottom: 1px;
    left: 10px;
    width: 86px;
    height: 35px;
}
.navigation a {
    -webkit-transition: opacity 0.2s linear;
    -moz-transition: opacity 0.2s linear;
    transition: opacity 0.2s linear;
    display: inline-block;
    width: 35px;
    height: 35px;
    text-indent: -9999px;
    overflow: hidden;
}
.navigation a.prev {
    background: url('data:image/png;base64, ') no-repeat 10px 10px;
    opacity: 0.5;
}
.navigation a.next {
    background: url('data:image/png;base64, ') no-repeat 10px 10px;
    opacity: 0.5;
}
.navigation a:hover {
    opacity: 1;
}
.navigation.first a.prev {
    opacity: 0.2;
}
.navigation.first a.prev:hover {
    opacity: 0.2;
}
.navigation.last a.next {
    opacity: 0.2;
}
.navigation.last a.next:hover {
    opacity: 0.2;
}
