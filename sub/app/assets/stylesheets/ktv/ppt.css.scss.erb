}
div.scrubber_holder {
    position: absolute;
    z-index: 3;
    padding: 14px 0;
    left: 95px;
    right: 80px;
    bottom: 0;
    height: 8px;
}
div.scrubber_holder.dragging {
    cursor: col-resize;
}
