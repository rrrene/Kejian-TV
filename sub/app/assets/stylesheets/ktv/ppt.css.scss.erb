.share #share {
    opacity: 1;
    z-index: 20;
}
#share .close {
    position: absolute;
    right: 0px;
    top: 0px;
    width: 30px;
    height: 30px;
    text-indent: -9999px;
    overflow: hidden;
    background: url("<%=asset_path 'close.png'%>") no-repeat center;
}
#share input[type=text] {
    height: 18px;
    padding: 3px 6px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    -ms-border-radius: 3px;
    -o-border-radius: 3px;
    border-radius: 3px;
    line-height: 12px;
    -webkit-box-shadow: inset rgba(0, 0, 0, 0.3) 0 2px 4px;
    -moz-box-shadow: inset rgba(0, 0, 0, 0.3) 0 2px 4px;
    box-shadow: inset rgba(0, 0, 0, 0.3) 0 2px 4px;
    -webkit-transition: background, color 0.2s linear;
    -moz-transition: background, color 0.2s linear;
    transition: background, color 0.2s linear;
    background: #ddd -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(0%,  #cccccc),  color-stop(100%,  #dddddd));
    background: #ddd -webkit-linear-gradient(#cccccc, #dddddd);
    background: #ddd -moz-linear-gradient(#cccccc, #dddddd);
    background: #ddd -o-linear-gradient(#cccccc, #dddddd);
    background: #ddd -ms-linear-gradient(#cccccc, #dddddd);
    background: #ddd linear-gradient(#cccccc, #dddddd);
    color: #333;
    text-shadow: #fff 0 1px 0;
    border: 1px solid #222;
    width: 291px;
    border: 1px solid rgba(0, 0, 0, 0.9);
    font-size: 12px;
}
#share input[type=text]:focus {
    background: #fff -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(0%,  #efefef),  color-stop(100%,  #ffffff));
    background: #fff -webkit-linear-gradient(#efefef, #ffffff);
    background: #fff -moz-linear-gradient(#efefef, #ffffff);
    background: #fff -o-linear-gradient(#efefef, #ffffff);
    background: #fff -ms-linear-gradient(#efefef, #ffffff);
    background: #fff linear-gradient(#efefef, #ffffff);
}
ul.choices {
    margin: 0 0 10px;
}
ul.choices li {
    display: inline;
}
ul.choices a {
    border: none;
    color: #fff;
    display: inline-block;
    font-size: 12px;
    height: 21px;
    text-align: center;
    line-height: 21px;
    padding: 0 10px;
    position: relative;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.7);
    white-space: nowrap;
    z-index: 1;
}
ul.choices a:hover {
    color: #fff;
}
ul.choices li.current a {
    background: #3CB579;
    background: #3cb579 -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(0%,  #3cb579),  color-stop(100%,  #004a4a));
    background: #3cb579 -webkit-linear-gradient(#3cb579, #004a4a);
    background: #3cb579 -moz-linear-gradient(#3cb579, #004a4a);
    background: #3cb579 -o-linear-gradient(#3cb579, #004a4a);
    background: #3cb579 -ms-linear-gradient(#3cb579, #004a4a);
    background: #3cb579 linear-gradient(#3cb579, #004a4a);
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    -ms-border-radius: 3px;
    -o-border-radius: 3px;
    border-radius: 3px;
    -webkit-box-shadow: rgba(0, 0, 0, 0.6) 0 0 0 1px, rgba(0, 0, 0, 0.4) 0 1px 3px, inset rgba(255, 255, 255, 0.4) 0 1px 0, inset rgba(0, 0, 0, 0.2) 0 -1px 0;
    -moz-box-shadow: rgba(0, 0, 0, 0.6) 0 0 0 1px, rgba(0, 0, 0, 0.4) 0 1px 3px, inset rgba(255, 255, 255, 0.4) 0 1px 0, inset rgba(0, 0, 0, 0.2) 0 -1px 0;
    box-shadow: rgba(0, 0, 0, 0.6) 0 0 0 1px, rgba(0, 0, 0, 0.4) 0 1px 3px, inset rgba(255, 255, 255, 0.4) 0 1px 0, inset rgba(0, 0, 0, 0.2) 0 -1px 0;
}
.selections {
    padding: 0 5px;
    margin: 0 0 10px;
}
.selections label {
    line-height: 24px;
    vertical-align: middle;
}
.selections input {
    vertical-align: middle;
    margin: 0 3px 0 0;
    padding: 0;
}
a {
    -webkit-transition: color 0.2s linear;
    -moz-transition: color 0.2s linear;
    transition: color 0.2s linear;
    text-decoration: none;
    color: #ccc;
}
a:hover {
    color: #fff;
}
.header p a {
    color: #3EC382;
}
.header p a:hover {
    color: #8AD3AF;
}
