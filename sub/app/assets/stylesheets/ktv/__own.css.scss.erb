.__own{
  .wrapper222 button {
      background: none;
      border: none;
      display: inline-block;
      margin: 0;
      padding: 0;
  }
   .wrapper222 button span{
      background: #29966b;
      background: -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(0%,  #3cb579),  color-stop(100%,  #004a4a));
      background: -webkit-linear-gradient(#3cb579,  #004a4a);
      background: -moz-linear-gradient(#3cb579,  #004a4a);
      background: -o-linear-gradient(#3cb579,  #004a4a);
      background: -ms-linear-gradient(#3cb579,  #004a4a);
      background: linear-gradient(#3cb579, #004a4a);
      -webkit-border-radius: 5px;
      -moz-border-radius: 5px;
      -ms-border-radius: 5px;
      -o-border-radius: 5px;
      border-radius: 5px;
      -webkit-box-shadow: inset 0 0 1px 0px rgba(255, 255, 255, 0.4), 0 1px 3px rgba(0, 0, 0, 0.3), 0 0 0 3px rgba(255, 255, 255, 0.1);
      -moz-box-shadow: inset 0 0 1px 0px rgba(255, 255, 255, 0.4), 0 1px 3px rgba(0, 0, 0, 0.3), 0 0 0 3px rgba(255, 255, 255, 0.1);
      box-shadow: inset 0 0 1px 0px rgba(255, 255, 255, 0.4), 0 1px 3px rgba(0, 0, 0, 0.3), 0 0 0 3px rgba(255, 255, 255, 0.1);
      border: 1px solid #033230;
      color: #fff;
      display: inline-block;
      font-size: 13px;
      height: 24px;
      text-align: center;
      line-height: 24px;
      padding: 1px 10px;
      vertical-align: middle;
      position: relative;
      text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.3);
      white-space: no-wrap;
      font-family: "Hiragino Sans GB", simsun, Helvetica, Arial, sans-serif;
      z-index: 1;
      margin: 5px 0;
  }  
  .wrapper222 button span:hover {
      background: #3cb579;
      color: #fff;
      background: #52c08c -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(0%,  #52c08c),  color-stop(100%,  #1b5957));
      background: #52c08c -webkit-linear-gradient(#52c08c,  #1b5957);
      background: #52c08c -moz-linear-gradient(#52c08c,  #1b5957);
      background: #52c08c -o-linear-gradient(#52c08c,  #1b5957);
      background: #52c08c -ms-linear-gradient(#52c08c,  #1b5957);
      background: #52c08c linear-gradient(#52c08c, #1b5957);
  }
  .wrapper222 button span:active {
    background: #29966b;
    background: -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(100%,  #004a4a), color-stop(0%,  #3cb579));
    background: -webkit-linear-gradient(#004a4a,  #3cb579);
    background: -moz-linear-gradient(#004a4a,  #3cb579);
    background: -o-linear-gradient(#004a4a,  #3cb579);
    background: -ms-linear-gradient(#004a4a,  #3cb579);
    background: linear-gradient(#004a4a,  #3cb579);
  }
